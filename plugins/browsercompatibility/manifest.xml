<?xml version="1.0"?>
<!DOCTYPE plugin SYSTEM "manifest.dtd">
<plugin version="2">
	<info>
		<version>0.3</version>
		<name>browsercompatibility</name>
		<title>Browser Compatibility Plugin</title>
		<author>Zarafa</author>
		<authorURL>http://www.zarafa.com</authorURL>
		<description>Browser Compatibility Testing</description>
	</info>
	<config>
		<configfile>config.php</configfile>
	</config>
	<components>
		<component>
			<files>
				<server>
					<serverfile>php/plugin.browsercompatibility.php</serverfile>
				</server>
				<client>
					<clientfile load="release">js/browsercompatibility.js</clientfile>
					<clientfile load="debug">js/browsercompatibility-debug.js</clientfile>
					<clientfile load="source">js/BrowserCompatibility.js</clientfile>
					<clientfile load="source">js/BrowserDetect.js</clientfile>
					<clientfile load="source">js/ABOUT.js</clientfile>
				</client>
			</files>
		</component>
	</components>
</plugin>

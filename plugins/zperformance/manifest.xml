<?xml version="1.0"?>
<!DOCTYPE plugin SYSTEM "manifest.dtd">
<plugin version="2">
	<info>
		<version>0.2</version>
		<name>zperformance</name>
		<title>Zarafa Performance Plugin</title>
		<author>Zarafa</author>
		<authorURL>http://www.zarafa.com</authorURL>
		<description>Assistence for measuring performance.</description>
	</info>
	<config>
		<configfile>config.php</configfile>
	</config>
	<components>
		<component>
			<files>
				<server>
					<serverfile>php/plugin.zperformance.php</serverfile>
				</server>
				<client>
					<clientfile load="release">js/zperformance.js</clientfile>
					<clientfile load="debug">js/zperformance-debug.js</clientfile>
					<clientfile load="source">js/ZPerformance.js</clientfile>
				</client>
			</files>
		</component>
	</components>
</plugin>

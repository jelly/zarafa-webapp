<?xml version="1.0"?>
<!DOCTYPE plugin SYSTEM "manifest.dtd">
<plugin version="2">
	<info>
		<version>1.2</version>
		<name>twidget</name>
		<title>Twidget</title>
		<author>Zarafa</author>
		<authorURL>http://www.zarafa.com</authorURL>
		<description>Display twitter feed of currently selected email</description>
	</info>
	<components>
		<component>
			<files>
				<client>
					<clientfile load="release">jsonp/jsonp.js</clientfile>
				</client>
			</files>
		</component>
		<component>
			<files>
				<client>
					<clientfile load="release">js/twidget.js</clientfile>
					<clientfile load="debug">js/twidget-debug.js</clientfile>
					<clientfile load="source">js/Twidget.js</clientfile>
					<clientfile load="source">js/ABOUT.js</clientfile>
				</client>
			</files>
		</component>
	</components>
</plugin>
